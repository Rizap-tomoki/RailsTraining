<div class="container mt-5">
  <div class="container mt-5">
    <div class="row justify-content-center">
      <div class="row justify-content-center mb-1">
        <div class="col-md-6">
          <div class="text-center">
            <h2 class="list-group-title text-center border border-secondary p-2 bg-secondary text-white">スキル一覧</h2>
          </div>
        </div>
      </div>
      <div class="col-md-6"> 
        <ul class="list-group">
          <% @skills.each do |skill| %>
            <li class="list-group-item text-center">
              <%= link_to skill.name, admin_skill_path(skill), class: "text-decoration-none" %>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</div>